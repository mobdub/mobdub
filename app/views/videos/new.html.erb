<h1>New video</h1>

<%= error_messages_for :video %>

<% form_for([@partner, @video]) do |f| %>
    <p>
      <b>Title</b><br/>
      <%= f.text_field :title %>
    </p>

    <p>
      <b>Uri</b><br/>
      <%= f.text_field :uri %>
    </p>

    <p>
      <b>Src</b><br/>
      <%= f.text_field :src %>
    </p>

    <p>
      <b>Thumb</b><br/>
      <%= f.text_field :thumb %>
    </p>

    <p>
      <b>Description</b><br/>
      <%= f.text_area :description %>
    </p>

    <p>
      <%= f.submit "Create" %>
    </p>
<% end %>

<%= link_to 'Back', videos_path %>
